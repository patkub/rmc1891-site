{
  "entrypoint": "index.html",
  "shell": "app/src/rmc-shell.html",
  "fragments": [
    "app/src/views/view-home.html",
    "app/src/views/view-about.html",
    "app/src/views/view-capabilities.html",
    "app/src/views/view-rapid-prototyping.html",
    "app/src/views/view-contact.html",
    "app/src/views/view-admin.html",
    "app/src/views/view-404.html"
  ],
  "sources": [
    "app/src/**/*",
    "app/images/**/*",
    "index.html",
    "manifest.json",
    ".htaccess"
  ],
  "extraDependencies": [
    "node_modules/bootstrap/dist/js/bootstrap.min.js",
    "node_modules/jquery/dist/jquery.slim.min.js",
    "node_modules/jquery/dist/jquery.slim.min.map",
    "node_modules/tether/dist/js/tether.min.js",
    "node_modules/popper.js/dist/umd/popper.min.js",
    "node_modules/popper.js/dist/umd/popper.min.js.map",
    "node_modules/font-awesome/fonts/*",
    "bower_components/webcomponentsjs/*"
  ],
  "lint": {
    "rules": ["polymer-2"],
    "ignoreWarnings": [
      "multiple-doc-comments",
      "set-unknown-attribute",
      "databind-with-unknown-property"
    ]
  }
}
