{
  "entrypoint": "index.html",
  "shell": "app/src/rmc-shell.html",
  "fragments": [
    "app/src/views/view-home.html",
    "app/src/views/view-about.html",
    "app/src/views/view-capabilities.html",
    "app/src/views/view-rapid-prototyping.html",
    "app/src/views/view-contact.html",
    "app/src/views/view-admin.html",
    "app/src/views/view-404.html"
  ],
  "sources": [
    "app/src/**/*",
    "app/images/**/*",
    "index.html"
  ],
  "extraDependencies": [
    "node_modules/bootstrap/dist/js/bootstrap.min.{js,js.map}",
    "node_modules/jquery/dist/jquery.slim.min.{js,map}",
    "node_modules/font-awesome/fonts/*",
    "bower_components/webcomponentsjs/*.{js,js.map}",
    "!bower_components/webcomponentsjs/gulpfile.js",
    "app/css/deferred.min.css",
    "app/js/deferred.min.js",
    "manifest.json",
    ".htaccess"
  ],
  "lint": {
    "rules": ["polymer-2"],
    "ignoreWarnings": [
      "databind-with-unknown-property"
    ]
  }
}
